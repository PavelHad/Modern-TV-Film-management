includes:
	- vendor/phpstan/phpstan-nette/extension.neon
	- phpstan-baseline.neon

parameters:
	paths:
		- app
		- tests

	level: 8

	ignoreErrors:
		- "#^Cannot call method render\\(\\) on Nette\\\\Application\\\\UI\\\\Template\\|stdClass\\.$#"
		- "#^Cannot call method add\\(\\) on Nette\\\\Application\\\\UI\\\\Template\\|stdClass\\.$#"
